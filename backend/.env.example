# Django Backend Environment Variables
DEBUG=True
SECRET_KEY=your-secret-key-here-change-in-production
ALLOWED_HOSTS=localhost,127.0.0.1

# Database Configuration
DB_NAME=geoanalysis
DB_USER=postgres
DB_PASSWORD=password
DB_HOST=localhost
DB_PORT=5432

# Earth Engine Configuration
EARTH_ENGINE_PROJECT_ID=your-earth-engine-project-id
EARTH_ENGINE_SERVICE_ACCOUNT_KEY=path/to/service-account-key.json

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# AI Assistant Configuration

# OpenAI Configuration (Optional - for enhanced AI responses)
# If not provided, the system will use rule-based responses
OPENAI_API_KEY=your_openai_api_key_here

# Alternative AI Providers (choose one)
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# AI Assistant Settings
AI_ASSISTANT_ENABLED=True
AI_ASSISTANT_MAX_TOKENS=500
AI_ASSISTANT_TEMPERATURE=0.7
AI_ASSISTANT_MODEL=gpt-3.5-turbo